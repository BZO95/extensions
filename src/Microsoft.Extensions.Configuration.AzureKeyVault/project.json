{
  "version": "1.0.0-*",
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "xmlDoc": true
  },
  "description": "Azure KeyVault configuration provider implementation for Microsoft.Extensions.Configuration.",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "https://github.com/aspnet/configuration"
    },
    "tags": [
      "configuration",
      "azure",
      "keyvault"
    ]
  },
  "dependencies": {
    "Microsoft.Azure.KeyVault": "1.0.0",
    "Microsoft.Extensions.Configuration": "1.1.0-*",
    "Microsoft.Extensions.Configuration.FileExtensions": "1.1.0-*",
    "Microsoft.IdentityModel.Clients.ActiveDirectory": "3.13.4",
    "NETStandard.Library": "1.6.1-*"
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Threading.Tasks": {
          "type": "build"
        },
        "System.Runtime": {
          "type": "build"
        }
      }
    }
  }
